package Model.Inventory;

/**
 *
 * @author Ramith
 */
public class Insert_Stock_Location {
    
private String stock_code;
private String stock_name;
private String stock_desc;

    public Insert_Stock_Location(String temp1, String temp2, String temp3){
        stock_code = temp1;
        stock_name = temp2;
        stock_desc = temp3;
    }
    

    /**
     * @return the stock_code
     */
    public String getStock_code() {
        return stock_code;
    }

    /**
     * @param stock_code the stock_code to set
     */
    public void setStock_code(String stock_code) {
        this.stock_code = stock_code;
    }

    /**
     * @return the stock_name
     */
    public String getStock_name() {
        return stock_name;
    }

    /**
     * @param stock_name the stock_name to set
     */
    public void setStock_name(String stock_name) {
        this.stock_name = stock_name;
    }

    /**
     * @return the stock_desc
     */
    public String getStock_desc() {
        return stock_desc;
    }

    /**
     * @param stock_desc the stock_desc to set
     */
    public void setStock_desc(String stock_desc) {
        this.stock_desc = stock_desc;
    }





}

    private void stock_locationActionPerformed(java.awt.event.ActionEvent evt) {                                               
        try {
            main_desktop_pane.removeAll();
            Stock_Management Stock_Management_obj = new Stock_Management();
            main_desktop_pane.add(Stock_Management_obj).setVisible(true);
        } catch (SQLException ex) {
            Logger.getLogger(Main_Menu.class.getName()).log(Level.SEVERE, null, ex);
        }

    }                                              

    private void inventory_typesActionPerformed(java.awt.event.ActionEvent evt) {                                                

        try {
            main_desktop_pane.removeAll();
            Inventory_Types Inventory_Types_obj = new Inventory_Types();
            main_desktop_pane.add(Inventory_Types_obj).setVisible(true);
        } catch (SQLException ex) {
            Logger.getLogger(Main_Menu.class.getName()).log(Level.SEVERE, null, ex);
        }

    }                                               

    private void unit_of_measureActionPerformed(java.awt.event.ActionEvent evt) {                                                

